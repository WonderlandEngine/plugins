        "tsc": "tsc --noEmit"
    },
    "prettier": "@wonderlandengine/prettier-config",
    "dependencies": {
        "@wonderlandcloud/cli": "^0.1.4",
        "@wonderlandengine/editor-api": "^1.3.1"
    },
    "devDependencies": {
        "@wonderlandengine/prettier-config": "^1.0.0",
        "typescript": "^5.6.3"
    }
}
