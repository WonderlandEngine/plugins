{
    "name": "@wonderlandengine/cleanup-plugin",
    "version": "0.0.2",
    "description": "Wonderland Editor plugin to cleanup resources in Wonderland Engine projects.",
    "main": "index.mjs",
    "typings": "index.d.ts",
    "type": "module",
    "module": "index.mjs",
    "keywords": [
        "wonderland-engine"
    ],
    "scripts": {
